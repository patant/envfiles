
# GIT
alias g="git status -s -b"
alias gc="git commit -v"
alias ga="git add -v"
alias gl="git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"


# unix
alias ll="ls -la"
alias fn='find . -name'
alias cdp='cd ~/dev/projects/'
alias cda='cd ~/dev/app/'

#VIM
alias v="~/bin/mvim --remote-silent"

#DEV
alias rubydev='export PS1="\[$BIBlue\]:\w\[$White\] \$\[$White\]\[$Cyan\] (\$(~/.rvm/bin/rvm-prompt))\[$Red\]\$(__git_ps1)\[$White\]\nâ€¢ "'

#Googe Chrome
alias chrome='open /Applications/Google\ Chrome.app'

#Akamai
alias akamaiCurl='curl -IXGET -H "Pragma: akamai-x-cache-on, akamai-x-cache-remote-on, akamai-x-check-cacheable, akamai-x-get-cache-key, akamai-x-get-extracted-values, akamai-x-get-nonces, akamai-x-get-ssl-client-session-id, akamai-x-get-true-cache-key, akamai-x-serial-no"'
